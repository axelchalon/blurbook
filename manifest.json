{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "script.js" ],
      "matches": [ "http://www.facebook.com/*", "https://www.facebook.com/*", "http://www.messenger.com/*", "https://www.messenger.com/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_extensionDescription__",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Blurbook",
   "options_page": "options.html",
   "permissions": [ "webRequest", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.13"
}
